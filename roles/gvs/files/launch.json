{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug ovs-vswitchd",
        "type": "cppdbg",
        "request": "attach",
        "processId": "${input:ovs-vswitchdProcessId}"
      },
      {
        "name": "Debug ovsdb",
        "type": "cppdbg",
        "request": "attach",
        "processId": "${input:ovsdbProcessId}"
      }
    ],
    "inputs": [
        {
            "id": "ovs-vswitchdProcessId",
            "type": "command",
            "command": "shellCommand.execute",
            "args": {
            "command": "pgrep ovs-vswitchd",
            "cwd": "${workspaceFolder}"
            }
        },
        {
            "id": "ovsdbProcessId",
            "type": "command",
            "command": "shellCommand.execute",
            "args": {
            "command": "pgrep ovsdb",
            "cwd": "${workspaceFolder}"
            }
        }
    ]
}